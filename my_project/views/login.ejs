<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/login.css' />
  </head>
  <body>
		<form action="" class="login_body" method="post">
			<div class="login_hd">
				<img src="../images/shopex.png" alt="">
			</div>
			<div class="center_wrap">
				<div class="title">
					<img src="../images/ecshop-logo.png" alt="">
				</div>
				<div class="login_start">
					<div class="login_input">
						<h3></h3>
						<div class="first">
							<input type="text" class="username" placeholder="用户名">
						</div>
						<div class="second">
							<input type="password" class="pwd" placeholder="密码">
						</div>
						<div class="third">
							<input type="text" class="code" placeholder="验证码">
						</div>
						<div class="last">
							<input type="submit" class="btn" value="登录">
						</div>
					</div>
				</div>
			</div>
		</form>
  </body>
</html>
